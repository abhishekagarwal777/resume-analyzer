
node_modules/

# Initialize git repository
git init

# Create .gitignore file
cat > .gitignore << EOF
# Dependencies
node_modules/
*/node_modules/

# Environment variables
.env
*/.env

# Build outputs
build/
dist/

# Logs
*.log
npm-debug.log*

# Runtime data
pids
*.pid
*.seed

# Coverage directory used by tools like istanbul
coverage/

# Mac
.DS_Store

# Windows
Thumbs.db

# Editor directories and files
.vscode/
.idea/
*.swp
*.swo
*~
